@model AccountViewModel

@{
    ViewData["Title"] = "Account Page";
}

<div class="text-center">
    <h1 class="display-4">Account (@Model.Name)</h1>
    <p>Friends:</p>
    @if (Model.FriendNames.Any())
    {
        @foreach (var item in Model.FriendNames)
        {
            <p> @item.</p>
        }

    }
    else
    {
        <p> Not friends.</p>
    }
</div>
